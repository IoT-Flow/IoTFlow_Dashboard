    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-hot-toast": "^2.4.0",
